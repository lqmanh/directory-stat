{
  "name": "directory-stat",
  "description": "Composable directory statistics fetcher where \"fs\" is insufficient",
  "license": "MIT",
  "version": "1.0.1",
  "author": "Luong Quang Manh @lqmanh",
  "main": "src/index.js",
  "bin": {
    "directory-stat": "./bin/run"
  },
  "scripts": {
    "build:docs": "esdoc"
  },
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "fast-glob": "^2.2.4"
  },
  "optionalDependencies": {
    "@oclif/plugin-warn-if-update-available": "^1.7.0"
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-node": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/*.js",
    "/bin",
    "/src"
  ],
  "repository": "github:lqmanh/directory-stat",
  "bugs": "https://github.com/lqmanh/directory-stat/issues",
  "homepage": "https://github.com/lqmanh/directory-stat",
  "keywords": [
    "dir",
    "directory",
    "stat",
    "statistics"
  ],
  "oclif": {
    "bin": "directory-stat",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7
    }
  }
}
